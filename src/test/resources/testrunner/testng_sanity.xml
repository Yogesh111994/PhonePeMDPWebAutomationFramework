<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" verbose="3" thread-count="3" parallel="tests">
	<listeners>
		<listener class-name="come.qa.phonePe.listener.ExtentReportListener"></listener>
		<listener class-name="come.qa.phonePe.listener.TestAllureListener"></listener>
	</listeners>

	<test name="Login page test">
		<classes>
			<class name="come.qa.phonePe.test.LoginPageTest" />
		</classes>
	</test>
		<test name="Home page test">
		<classes>
			<class name="come.qa.phonePe.test.HomePageTest" />
		</classes>
	</test>
	
	
</suite>